{
	"name": "flarum-lang/kannada",
	"type": "flarum-extension",
	"description": "Kannada language pack for Flarum.",
	"homepage": "https://github.com/flarum-lang/kannada",
	"keywords": [
		"locale",
		"flarum",
		"flarum-locale",
		"kn-locale",
		"kannada-locale",
		"kannada"
	],
	"license": "MIT",
	"support": {
		"issues": "https://github.com/flarum-lang/kannada/issues",
		"source": "https://github.com/flarum-lang/kannada"
	},
	"require": {
		"flarum/core": "^1.6"
	},
	"minimum-stability": "dev",
	"prefer-stable": true,
	"extra": {
		"flarum-extension": {
			"title": "Kannada",
			"category": "language",
			"icon": {
				"image": "icon.svg",
				"backgroundSize": "cover",
				"backgroundPosition": "center"
			}
		},
		"flarum-locale": {
			"code": "kn",
			"title": "Kannada"
		},
		"extiverse": {
			"discuss": ""
		}
	}
}
